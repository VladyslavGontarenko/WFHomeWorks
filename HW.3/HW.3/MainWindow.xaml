<Window x:Class="HW._3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HW._3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Canvas>
            <Ellipse Fill="White" Stroke="Black" Width="210" Height="210" Margin="5" Canvas.Top="95" Canvas.Left="95">
            </Ellipse>
            <Ellipse Fill="Red" Stroke="White" Width="200" Height="200" Margin="5" Canvas.Top="100" Canvas.Left="100">
            </Ellipse>
            <Rectangle Fill="White" RadiusX="5" RadiusY="5" Width="140" Height="35" Margin="5" Canvas.Top="180" Canvas.Left="130">
            </Rectangle>
        </Canvas>
    </Grid>
</Window>
